/* miniprogram/pages/parse/index.wxss */

page {
  background-color: #f8f8f8;
  height: 100%;
}

.parse {
padding: 10rpx;
}
.section {
  padding: 30rpx;
  margin: 20rpx 10rpx 40rpx;
  background-color: #fff;
  border-radius: 40rpx;
}
.section>view {
  font-weight: 400;
}
button.clear{
  float: right;
  font-weight: 300;
  width: 100px !important;
  padding: 0 !important;
  color: #07c160;
  background: #fff;
  line-height: 1;
  font-size: 14px;
}

button.clear::after {
  content: none;
}

.url-textarea {
  margin-top: 20rpx;
  background-color: #f5f5f5;
  box-sizing: border-box;
  padding: 20rpx;
  min-height: 300rpx;
  width: 100%;
}

.subBut {
  width: 80%;
  margin: 0 auto;
}


.wrapper{
    position: relative;
}

.wrapper image{
  width: 100vw;
    height: 100vh;
}

.wrapper text{
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  text-align: center;
  width: 100%;
  color: #fff;
  font-size: 200rpx;
}